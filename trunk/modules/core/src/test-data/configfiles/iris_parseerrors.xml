<system-config name="iris" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="C:\gepo\mycvsprojects\iris\src\conf\xml\iris.xsd">
	<global-settingssssssssssssssss>
		<archivehandler>
			<cronintervall>0 30 9 * * ?</cronintervall>
		</archivehandler>
		<errorhandler>
			<cronintervall>0 30 9 * * ?</cronintervall>
		</errorhandler>
		<emailserversettings>
			<smtp>
				<smtpserver id="1">tdvmail</smtpserver>
				<smtpserver id="2">tdvmail2</smtpserver>
			</smtp>
			<pop>
				<popserver id="1">tdvmail</popserver>
				<popserver id="2">tdvmail2</popserver>
			</pop>
			<imap>
				<imapserver id="1">tdvmail</imapserver >
				<imapserver id="2">tdvmail2</imapserver >
			</imap>
		</emailserversettings>
		<servicerootpath>c:/tradevision/</servicerootpath>
	</global-settings>
	
	<service type="poll-ejb" id="poll-pnl2k" usesrelativerootpath="false">
		<poll>
			<message>are you alive?</message>
			<intervall>5000</intervall>
		</poll>
		<error errorhandleron="true">
			<errorfolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/error/</errorfolderpath>
			<sortonresend>false</sortonresend>			
			<errornotification errornotificationon="false" >
				<inactivitytime>0</inactivitytime>
				<numberoferrors>10000</numberoferrors>
					<emailnotification>
					<emailonfailure>c:/tradevision/iris/services/pnl/poll-pnl2k/template/pnldown.xml</emailonfailure>
					<emailservice>c:/tradevision/iris/services/pnl/pnl-email/in/</emailservice>
				</emailnotification>
				<sms>
					<smsonfailure>c:/tradevision/iris/services/pnl/poll-pnl2k/template/pnldown.xml</smsonfailure>
					<smsservice>c:/tradevision/iris/services/pnl/pnl-email/in/</smsservice>
				</sms>	
			</errornotification>
		</error>
		<nameservice>
			<iorpath>C:/tradevision/iris/conf/pnl2000.ior</iorpath>
			<objectname>Alive</objectname>
			<objectmethod>isAlive</objectmethod>
			<paramtype>String</paramtype>
		</nameservice>
		<logging>
			<appender type="org.apache.log4j.RollingFileAppender">
				<maxfilesize>100KB</maxfilesize>
				<maxbackupindex>10</maxbackupindex>
			</appender>
			<logpattern>%d{yyyy-MM-dd HH:mm:ss};%p;%t;%x;%m;%n</logpattern>
			<logfolderpath>C:/tradevision/iris/services/pnl/poll-pnl2k/log/</logfolderpath>
		</logging>
	</service>


