<?xml version="1.0" encoding="utf-8"?>
<system-config name="iris" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="C:/gepo/mycvsprojects/iris/modules/xml-binding/src/schema/iris.xsd">
	<global-settings>
		<archivehandler>
			<cronintervall>Every 5 minutes@0 1/5 * * * ?</cronintervall>
		</archivehandler>
		<emailserversettings sendintervallInMilliSeconds="30000">
			<smtp>
				<smtpserver id="1">tdvmail1</smtpserver>
				<smtpserver id="2">tdvmail2</smtpserver>
			</smtp>
			<pop>
				<popserver id="1">tdvmail</popserver>
				<popserver id="2">tdvmail2</popserver>
			</pop>
			<imap>
				<imapserver id="1">tdvmail</imapserver>
				<imapserver id="2">tdvmail2</imapserver>
			</imap>
		</emailserversettings>
		<servicerootpath>c:/tradevision/</servicerootpath>
	</global-settings>

	<service type="file-ejb" id="arinc_ba-c2k" createuniquename="true" usesrelativerootpath="false">
		<infolder>
			<infolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/in/</infolderpath>
			<intervallInMilliSeconds>5000</intervallInMilliSeconds>
			<skipfirstblankline>true</skipfirstblankline>
			<sortalpha>true</sortalpha>
			<infoldersendchunks sendinfolderchunkson="false">
				<infolderchunksize>5</infolderchunksize>
				<infolderchunkthreshold>10</infolderchunkthreshold>
			</infoldersendchunks>
		</infolder>
		<backup backuphandleron="true" archiveron="true">
			<backupfolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/backup/</backupfolderpath>
			<archivefolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/archived/</archivefolderpath>
		</backup>
		<transform transformon="false"/>
		<error errorhandleron="true">
			<errorfolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/error/</errorfolderpath>
			<errorinfofolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/error_info/</errorinfofolderpath>
			<sortonresend>false</sortonresend>
			<cronintervall>Every 5 minutes@0 1/5 * * * ?</cronintervall>
			<errornotification errornotificationon="true">
				<notifyeveryintervallInMilliSeconds>60000</notifyeveryintervallInMilliSeconds>
				<numberoferrors>1</numberoferrors>
				<emailnotification>
                    <emailonfailure>C:/Tradevision/iris/services/c2k/arinc_ba-c2k/template/erroronfailure.xml</emailonfailure>
				</emailnotification>
				<sms>
					<smsonfailure>c:/tradevision/iris/services/pnl/poll-pnl2k/template/pnldown.xml</smsonfailure>
					<smsservice>c:/tradevision/iris/services/pnl/pnl-email/in/</smsservice>
				</sms>
			</errornotification>
		</error>
        <inactivity inactivityOn="true">
            <maxInactivityPeriodInMilliSeconds>30000</maxInactivityPeriodInMilliSeconds>
            <emailOnInactivity>C:/Tradevision/iris/services/c2k/arinc_ba-c2k/template/erroronincativity.xml</emailOnInactivity>
        </inactivity>
		<nameservice>
			<iorpath>C:/tradevision/iris/conf/cargo2000net.ior</iorpath>
			<objectname>MessageReceiverBean</objectname>
			<objectmethod>processMessage</objectmethod>
			<paramtype>String</paramtype>
			<reconnectToObjectInMilliSeconds>10000</reconnectToObjectInMilliSeconds>
			<reconnectToObjectPolicy>reconnectOnNewMessage</reconnectToObjectPolicy>
		</nameservice>
		<applicationlogger>
			<appender type="RollingFileAppender">
				<maxfilesize>100KB</maxfilesize>
				<maxbackupindex>10</maxbackupindex>
			</appender>
			<logpattern>%d{yyyy-MM-dd HH:mm:ss};%p;%t;%x;%m%n</logpattern>
			<logfolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/log/</logfolderpath>
			<loglevel>DEBUG</loglevel>
		</applicationlogger>
		<messagelogger>
			<appender type="DailyRollingFileAppender">
				<rollingschedule>'.'yyyy-MM-dd</rollingschedule>
			</appender>
			<logpattern>%d{yyyy-MM-dd HH:mm:ss};%p;%t;%x;%m%n</logpattern>
			<logfolderpath>c:/tradevision/iris/services/c2k/arinc_ba-c2k/log/</logfolderpath>
			<loglevel>DEBUG</loglevel>
		</messagelogger>
	</service>
